<p id="notice"><%= notice %></p>

<h1 class="page-header">Events Feed</h1>



<% @events.each do |event| %>
	<% if lookup_context.template_exists?(event.action, "events/activities", true) %>
		<%= render partial: "events/activities/#{event.action}", locals: {event: event} %><br>
  <% end %>
  <% end %>
<br>
